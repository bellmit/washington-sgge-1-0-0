 <select id="fetchAllAdvogados" resultMap="advogadoResult">
    SELECT id,
 <include refid="alladvogadoColumns" />
   FROM advogado ORDER BY id ASC
 </select>

 <select id="fetchAdvogadoById" parameterType="Integer" resultMap="advogadoResult">
    SELECT id,
 <include refid="alladvogadoColumns" />
   FROM advogado WHERE id = #{fetchid}
 </select>

 <select id="fetchAllAdvogadosRequest" parameterType="PagedInquiryRequest" resultMap="advogadoResult">

     SELECT id,<include refid="alladvogadoColumns" />
           FROM advogado
           where 0 = 0
           <if test="permissaoTypeEnumValue == 1">
               and id IS NOT NULL
            </if>
           <if test="permissaoTypeEnumValue != 1">
               <if test="emprId == null">
                   <if test="emprIds != null">
                       and emprId in

                     <foreach item="item" index="index" collection="emprIds"
                          open="(" separator="," close=")">
                         #{item}
                     </foreach>

                 </if>
             </if>
               <if test="emprId != null"> and emprId = #{emprId}</if>
           </if>
           ORDER BY id ASC
</select>

<select id="fetchAdvogadoRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM advogado WHERE id IS NOT NULL <if test="emprId != null"> and emprId = emprId</if>

 </select>

