 <select id="fetchAllProdutoEmpresas" resultMap="ProdutoEmpresaResult">
    SELECT id,
 <include refid="allProdutoEmpresaColumns" />
   FROM produtoempresa ORDER BY id ASC
 </select>

 <select id="fetchProdutoEmpresaById" parameterType="Integer" resultMap="ProdutoEmpresaResult">
    SELECT id,
 <include refid="allProdutoEmpresaColumns" />
   FROM produtoempresa WHERE id = #{fetchid}
 </select>

 <select id="fetchAllProdutoEmpresasRequest" parameterType="ProdutoEmpresaInquiryRequest" resultMap="ProdutoEmpresaResult">

     SELECT id,<include refid="allProdutoEmpresaColumns" />
           FROM produtoempresa where 0 = 0 <if test="criteria.emprId != null"> and emprId = criteria.emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

<select id="fetchProdutoEmpresaRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM produtoempresa WHERE id IS NOT NULL <if test="criteria.emprId != null"> and emprId = criteria.emprId</if>

 </select>

 <insert id="insertProdutoEmpresa" parameterType="ProdutoEmpresa" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO produtoempresa (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allProdutoEmpresaColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allProdutoEmpresaValues" /> )
 </insert>

 <update id="updateProdutoEmpresa" parameterType="ProdutoEmpresa">
   UPDATE produtoempresa SET
<if test="prodId!= null">prodId = #{prodId.id},</if>
<if test="informAdicionaisParaNFe!= null">informAdicionaisParaNFe = #{informAdicionaisParaNFe},</if>
<if test="anotainternas!= null">anotainternas = #{anotainternas},</if>
