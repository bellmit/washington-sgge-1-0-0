      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="ConfigFiscalResult" type="ConfigFiscal">
    <result property="id" column="id"/>
    <collection property="princAtividade" column="princAtividade" select="RegimeMap.fetchRegimeById"/>
    <collection property="regime" column="regime" select="RegimeMap.fetchRegimeById"/>
    <result property="aliqSimples" column="aliqSimples"/>
<result property="parentId" column="parentId"/>
<result property="tabelaEnumValue" column="tabelaEnumValue"/>
<result property="emprId" column="emprId"/>
<result property="processId" column="processId"/>
<result property="createUser" column="create_user"/>
<result property="createDateUTC" column="create_date"/>
<result property="modifyUser" column="modify_user"/>
<result property="modifyDateUTC" column="modify_date"/>
  </resultMap>

  <sql id="allConfigFiscalColumns">
,princAtividade
,regime
,aliqSimples
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user

</sql>
<sql id="allConfigFiscalValues">
#{princAtividade.id},
#{regime.id},
#{aliqSimples},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser}
  </sql>


 <select id="fetchAllConfigFiscals" resultMap="ConfigFiscalResult">
    SELECT id,
 <include refid="allConfigFiscalColumns" />
   FROM configfiscal ORDER BY id ASC
 </select>

 <select id="fetchConfigFiscalById" parameterType="Integer" resultMap="ConfigFiscalResult">
    SELECT id,
 <include refid="allConfigFiscalColumns" />
   FROM configfiscal WHERE id = #{fetchid}
 </select>

 <select id="fetchAllConfigFiscalsRequest" parameterType="ConfigFiscalInquiryRequest" resultMap="ConfigFiscalResult">

     SELECT id,<include refid="allConfigFiscalColumns" />
           FROM configfiscal where 0 = 0 <if test="criteria.emprId != null"> and emprId = criteria.emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

<select id="fetchConfigFiscalRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM configfiscal WHERE id IS NOT NULL <if test="criteria.emprId != null"> and emprId = criteria.emprId</if>

 </select>

 <insert id="insertConfigFiscal" parameterType="ConfigFiscal" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO configfiscal (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allConfigFiscalColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allConfigFiscalValues" /> )
 </insert>

 <update id="updateConfigFiscal" parameterType="ConfigFiscal">
   UPDATE configfiscal SET
<if test="princAtividade!= null">princAtividade = #{princAtividade.id},</if>
<if test="regime!= null">regime = #{regime.id},</if>
<if test="aliqSimples!= null">aliqSimples = #{aliqSimples},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteConfigFiscalById" parameterType="ConfigFiscal">
  DELETE FROM configfiscal WHERE id = #{id}
 </delete>

 <delete id="deleteAllConfigFiscals">
 DELETE FROM configfiscal
  </delete>
