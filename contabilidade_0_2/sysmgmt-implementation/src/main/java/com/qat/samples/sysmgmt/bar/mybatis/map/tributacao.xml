<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="TributacaoMap">
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache" />
	<cache type="org.mybatis.caches.ehcache.EhcacheCache" />

	<!-- Result maps describe the mapping between the columns returned from
		a query, and the class properties. A result map isnt necessary if the columns
		(or aliases) match to the properties exactly. -->
	<resultMap id="TributacaoResult" type="Tributacao">
		<result property="id" column="id" />
		<result property="descricao" column="descricao" />
		<result property="tipo" column="tipo" />
		<result property="parentId" column="parentId" />
		<result property="tabelaEnumValue" column="tabelaEnumValue" />
		<result property="emprId" column="emprId" />
		<result property="processId" column="processId" />
		<result property="createUser" column="create_user" />
		<result property="createDateUTC" column="create_date" />
		<result property="modifyUser" column="modify_user" />
		<result property="modifyDateUTC" column="modify_date" />
		<result property="prodId" column="prodId" />
		<collection property="imposto" column="imposto"
			select="NFNotaInfoItemImpostoMap.fetchNFNotaInfoItemImpostoById" />
		<collection property="impostoDevolvido" column="impostoDevolvido"
			select="NFImpostoDevolvidoMap.fetchNFImpostoDevolvidoById" />
		<collection property="cfop" column="cfop" select="CfopMap.fetchCfopById" />
	</resultMap>

	<sql id="allTributacaoColumns">
		descricao
		,tipo
		,prodId
		,cfop
		,imposto
		,impostoDevolvido
		,parentId
		,tabelaEnumValue
		,emprId
		,processId
		,create_date
		,create_user
		,modify_date
		,modify_user

	</sql>
	<sql id="allTributacaoValues">
		#{descricao},
		#{tipo},
		#{prodId},
		#{cfop.id},
		#{imposto.id},
		#{impostoDevolvido.id},
		#{parentId},
		#{tabelaEnumValue},
		#{emprId},
		#{processId},
		#{createDateUTC},
		#{createUser},
		#{modifyDateUTC},
		#{modifyUser}
	</sql>


	<select id="fetchAllTributacaos" resultMap="TributacaoResult">
		SELECT id,
		<include refid="allTributacaoColumns" />
		FROM tributacao ORDER BY id ASC
	</select>

	<select id="fetchTributacaoById" parameterType="Integer"
		resultMap="TributacaoResult">
		SELECT id,
		<include refid="allTributacaoColumns" />
		FROM tributacao WHERE id = #{fetchid}
	</select>

	<select id="fetchAllTributacaosRequest" parameterType="TributacaoInquiryRequest"
		resultMap="TributacaoResult">

		SELECT id,
		<include refid="allTributacaoColumns" />
		FROM tributacao WHERE id IS NOT NULL
		<if test="permissaoTypeEnumValue == 1">
			and id IS NOT NULL
		</if>
		<if test="permissaoTypeEnumValue != 1">
			<if test="emprId == null">
				<if test="emprIds != null"> and emprId in

				<foreach item="item" index="index" collection="emprIds"
		             open="(" separator="," close=")">
		        #{item}
		    	</foreach>

				</if>
			</if>
			<if test="emprId != null"> and emprId = #{emprId}</if>
		</if>
		ORDER BY descricao ASC

	</select>

	<select id="fetchTributacaoRowCount" resultType="Integer">
		SELECT COUNT(*) AS RECORD_COUNT FROM tributacao WHERE id IS NOT NULL
		<if test="permissaoTypeEnumValue == 1">
			and id IS NOT NULL
		</if>
		<if test="permissaoTypeEnumValue != 1">
			<if test="emprId == null">
				<if test="emprIds != null"> and emprId in

				<foreach item="item" index="index" collection="emprIds"
		             open="(" separator="," close=")">
		        #{item}
		    	</foreach>

				</if>
			</if>
			<if test="emprId != null"> and emprId = #{emprId}</if>
		</if>

	</select>

	<insert id="insertTributacao" parameterType="Tributacao"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO tributacao (
		<if test="id != null">
			<if test="id > 0">id,</if>
		</if>
		<include refid="allTributacaoColumns" />
		)
		VALUES (
		<if test="id != null">
			<if test="id > 0">#{id},</if>
		</if>
		<include refid="allTributacaoValues" />
		)
	</insert>

	<update id="updateTributacao" parameterType="Tributacao">
		UPDATE tributacao SET
		<if test="descricao!= null">descricao = #{descricao},</if>
		<if test="tipo!= null">tipo = #{tipo},</if>
		<if test="prodId!= null">prodId = #{prodId},</if>
		<if test="cfop != null">cfop = #{cfop.id},</if>
		<if test="imposto!= null">imposto = #{imposto.id},</if>
		<if test="impostoDevolvido!= null">impostoDevolvido = #{impostoDevolvido.id},</if>
		<if test="parentId!= null"> parentId=#{parentId},</if>
		<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
		<if test="emprId!= null"> emprId=#{emprId},</if>
		<if test="processId!= null"> processId=#{processId},</if>
		<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
		<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
		WHERE
		id = #{id}
	</update>

	<delete id="deleteTributacaoById" parameterType="Tributacao">
		DELETE FROM tributacao WHERE id = #{id}
	</delete>

	<delete id="deleteAllTributacaos">
		DELETE FROM tributacao
	</delete>
</mapper>


