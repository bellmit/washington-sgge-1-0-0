<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ConfiguracaoMap">
 <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
 <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

  <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="ConfiguracaoResult" type="Configuracao">
    <result property="id" column="id"/>

<result property="parentId" column="parentId"/>
<result property="tabelaEnumValue" column="tabelaEnumValue"/>
<result property="emprId" column="emprId"/>
<result property="processId" column="processId"/>
<result property="createUser" column="create_user"/>
<result property="createDateUTC" column="create_date"/>
<result property="modifyUser" column="modify_user"/>
<result property="modifyDateUTC" column="modify_date"/>
    <collection property="confGeral" column="confGeral" select="RegimeMap.fetchRegimeById"/>
    <collection property="confFiscal" column="confFiscal" select="RegimeMap.fetchRegimeById"/>
    <collection property="confProd" column="confProd" select="RegimeMap.fetchRegimeById"/>
    <collection property="confVendas" column="confVendas" select="RegimeMap.fetchRegimeById"/>
    <collection property="confCMTP" column="confCMTP" select="RegimeMap.fetchRegimeById"/>
    <collection property="confEntrada" column="confEntrada" select="RegimeMap.fetchRegimeById"/>
    <collection property="confCarne" column="confCarne" select="RegimeMap.fetchRegimeById"/>
    <collection property="confNFe" column="confNFe" select="RegimeMap.fetchRegimeById"/>
<collection property="boletoList" column="id" select="DocumentoMap.fetchDocumentoByEmpresa"/>
  </resultMap>

  <sql id="allConfiguracaoColumns">
confGeral
,confFiscal
,confProd
,confVendas
,confCMTP
,confEntrada
,confCarne
,confNFe
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user

</sql>
<sql id="allConfiguracaoValues">
#{confGeral.id},
#{confFiscal.id},
#{confProd.id},
#{confVendas.id},
#{confCMTP.id},
#{confEntrada.id},
#{confCarne.id},
#{confNFe.id},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser}
  </sql>


 <select id="fetchAllConfiguracaos" resultMap="ConfiguracaoResult">
    SELECT id,
 <include refid="allConfiguracaoColumns" />
   FROM configuracao ORDER BY id ASC
 </select>

 <select id="fetchConfiguracaoById" parameterType="Integer" resultMap="ConfiguracaoResult">
    SELECT id,
 <include refid="allConfiguracaoColumns" />
   FROM configuracao WHERE id = #{fetchid}
 </select>

 <select id="fetchAllConfiguracaosRequest" parameterType="PagedInquiryRequest" resultMap="ConfiguracaoResult">

     SELECT id,<include refid="allConfiguracaoColumns" />
           FROM configuracao where 0 = 0 <if test="emprId != null"> and emprId = emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

<select id="fetchConfiguracaoRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM configuracao WHERE id IS NOT NULL <if test="emprId != null"> and emprId = emprId</if>

 </select>

 <insert id="insertConfiguracao" parameterType="Configuracao" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO configuracao (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allConfiguracaoColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allConfiguracaoValues" /> )
 </insert>

 <update id="updateConfiguracao" parameterType="Configuracao">
   UPDATE configuracao SET
<if test="confGeral!= null">confGeral = #{confGeral.id},</if>
<if test="confFiscal!= null">confFiscal = #{confFiscal.id},</if>
<if test="confProd!= null">confProd = #{confProd.id},</if>
<if test="confVendas!= null">confVendas = #{confVendas.id},</if>
<if test="confCMTP!= null">confCMTP = #{confCMTP.id},</if>
<if test="confEntrada!= null">confEntrada = #{confEntrada.id},</if>
<if test="confCarne!= null">confCarne = #{confCarne.id},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteConfiguracaoById" parameterType="Configuracao">
  DELETE FROM configuracao WHERE id = #{id}
 </delete>

 <delete id="deleteAllConfiguracaos">
 DELETE FROM configuracao
  </delete>
</mapper>


