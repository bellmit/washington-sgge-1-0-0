<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ConfiguracaoMap">
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache" />
	<cache type="org.mybatis.caches.ehcache.EhcacheCache" />

	<!-- Result maps describe the mapping between the columns returned from
		a query, and the class properties. A result map isnt necessary if the columns
		(or aliases) match to the properties exactly. -->
	<resultMap id="ConfiguracaoResult" type="Configuracao">
		<result property="id" column="id" />
		<result property="parentId" column="parentId" />
		<result property="tabelaEnumValue" column="tabelaEnumValue" />
		<result property="emprId" column="emprId" />
		<result property="processId" column="processId" />
		<result property="confCabecalho" column="confCabecalho" />
		<result property="createUser" column="create_user" />
		<result property="createDateUTC" column="create_date" />
		<result property="modifyUser" column="modify_user" />
		<result property="modifyDateUTC" column="modify_date" />
		<collection property="confGeral" column="confGeral"
			select="ConfigGeralMap.fetchConfigGeralById" />
		<collection property="confFiscal" column="confFiscal"
			select="ConfigFiscalMap.fetchConfigFiscalById" />
		<collection property="confProd" column="confProd"
			select="ConfigProdutoMap.fetchConfigProdutoById" />
		<collection property="confVendas" column="confVendas"
			select="ConfigVendasMap.fetchConfigVendasById" />
		<collection property="confSMTP" column="confSMTP"
			select="ConfigSMTPMap.fetchConfigSMTPById" />
		<collection property="confEntrada" column="confEntrada"
			select="ConfigEntradaMap.fetchConfigEntradaById" />
		<collection property="confCarne" column="confCarne"
			select="ConfigCarneMap.fetchConfigCarneById" />
		<collection property="confNFe" column="confNFe"
			select="ConfiguracaoNFeMap.fetchConfiguracaoNFeById" />
		<collection property="configOS" column="configOS"
			select="ConfigOSMap.fetchConfigOSById" />
		<collection property="confAlertas" column="configOS"
			select="ConfigAlertasMap.fetchConfigAlertasById" />
		<!-- <collection property="confBlueSoft" column="confBlueSoft" select="ConfiguracaoNFeMap.fetchConfiguracaoNFeById"/> -->
		<collection property="boletoList" column="id"
			select="BoletoMap.fetchBoletoByEmprId" />
	</resultMap>

	<sql id="allConfiguracaoColumns">
		confCabecalho
		,confGeral
		,confFiscal
		,confProd
		,confVendas
		,confSMTP
		,confEntrada
		,confCarne
		,confNFe
		,configOS
		,confAlertas
		,confBlueSoft
		,parentId
		,tabelaEnumValue
		,emprId
		,processId
		,create_date
		,create_user
		,modify_date
		,modify_user


	</sql>
	<sql id="allConfiguracaoValues">
		#{confCabecalho},
		#{confGeral.id},
		#{confFiscal.id},
		#{confProd.id},
		#{confVendas.id},
		#{confSMTP.id},
		#{confEntrada.id},
		#{confCarne.id},
		#{confNFe.id},
		#{configOS.id},
		#{confAlertas.id},
		#{confBlueSoft.id},
		#{parentId},
		#{tabelaEnumValue},
		#{emprId},
		#{processId},
		#{createDateUTC},
		#{createUser},
		#{modifyDateUTC},
		#{modifyUser}

	</sql>


	<select id="fetchAllConfiguracaos" resultMap="ConfiguracaoResult">
		SELECT id,
		<include refid="allConfiguracaoColumns" />
		FROM configuracao ORDER BY id ASC
	</select>

	<select id="fetchConfiguracaoById" parameterType="Integer"
		resultMap="ConfiguracaoResult">
		SELECT id,
		<include refid="allConfiguracaoColumns" />
		FROM configuracao WHERE id = #{fetchid}
	</select>

	<select id="fetchAllConfiguracaosRequest" parameterType="PagedInquiryRequest"
		resultMap="ConfiguracaoResult">

		SELECT id,
		<include refid="allConfiguracaoColumns" />
		FROM configuracao where 0 = 0
		<if test="emprId != null"> and emprId = emprId</if>
		ORDER BY id ASC

	</select>

	<select id="fetchConfiguracaoRowCount" resultType="Integer">
		SELECT COUNT(*) AS RECORD_COUNT FROM configuracao WHERE id IS NOT NULL
		<if test="emprId != null"> and emprId = emprId</if>

	</select>

	<insert id="insertConfiguracao" parameterType="Configuracao"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO configuracao (
		<if test="id != null">
			<if test="id > 0">id,</if>
		</if>
		<include refid="allConfiguracaoColumns" />
		)
		VALUES (
		<if test="id != null">
			<if test="id > 0">#{id},</if>
		</if>
		<include refid="allConfiguracaoValues" />
		)
	</insert>

	<update id="updateConfiguracao" parameterType="Configuracao">
		UPDATE configuracao SET
		<if test="confGeral!= null">confGeral = #{confGeral.id},</if>
		<if test="confFiscal!= null">confFiscal = #{confFiscal.id},</if>
		<if test="confProd!= null">confProd = #{confProd.id},</if>
		<if test="confVendas!= null">confVendas = #{confVendas.id},</if>
		<if test="confSMTP!= null">confSMTP = #{confSMTP.id},</if>
		<if test="confEntrada!= null">confEntrada = #{confEntrada.id},</if>
		<if test="confCarne!= null">confCarne = #{confCarne.id},</if>
		<if test="confNFe!= null">confNFe = #{confNFe.id},</if>
		<if test="configOS!= null">configOS = #{configOS.id},</if>
		<if test="confAlertas!= null">confAlertas = #{confAlertas.id},</if>
		<if test="confBlueSoft!= null">confBlueSoft = #{confBlueSoft.id},</if>
		<if test="parentId!= null"> parentId=#{parentId},</if>
		<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
		<if test="emprId!= null"> emprId=#{emprId},</if>
		<if test="processId!= null"> processId=#{processId},</if>
		<if test="confCabecalho!= null"> confCabecalho=#{confCabecalho},</if>
		<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
		<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
		WHERE
		id = #{id}
	</update>

	<delete id="deleteConfiguracaoById" parameterType="Configuracao">
		DELETE FROM configuracao WHERE id = #{id}
	</delete>

	<delete id="deleteAllConfiguracaos">
		DELETE FROM configuracao
	</delete>
</mapper>


