<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProdutoMap">
 <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
 <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

  <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="ProdutoResult" type="Produto">
    <result property="id" column="id"/>
    <result property="cdBarras" column="cdBarras"/>
    <result property="ncm" column="ncm"/>
    <result property="cEST" column="cEST"/>
    <result property="produto" column="produto"/>
    <result property="quant" column="quant"/>
    <result property="dataCreate" column="dataCreate" />
	<result property="parentId" column="parentId"/>
	<result property="tabelaEnumValue" column="tabelaEnumValue"/>
	<result property="emprId" column="emprId"/>
	<result property="processId" column="processId"/>
	<result property="createUser" column="create_user"/>
	<result property="createDateUTC" column="create_date"/>
	<result property="modifyUser" column="modify_user"/>
	<result property="modifyDateUTC" column="modify_date"/>
	<collection property="uniMed" column="uniMed" select="UniMedMap.fetchUniMedById"/>
	<collection property="marca" column="marca" select="MarcaMap.fetchMarcaById"/>
	<collection property="excTabIPI" column="excTabIPI" select="DoisValorMap.fetchDoisValorById"/>
	<collection property="origem" column="origem" select="DoisValorMap.fetchDoisValorById"/>
	<collection property="produtoTipo" column="produtoTipo" select="DoisValorMap.fetchDoisValorById"/>
	<collection property="medicamento" column="medicamento" select="NFNotaInfoItemProdutoMedicamentoMap.fetchNFNotaInfoItemProdutoMedicamentoById"/>
	<collection property="veiculo" column="veiculo" select="NFNotaInfoItemProdutoVeiculoMap.fetchNFNotaInfoItemProdutoVeiculoById"/>
	<collection property="armamento" column="armamento" select="NFNotaInfoItemProdutoArmamentoMap.fetchNFNotaInfoItemProdutoArmamentoById"/>
	<collection property="combustivel" column="combustivel" select="NFNotaInfoItemProdutoCombustivelMap.fetchNFNotaInfoItemProdutoCombustivelById"/>

  </resultMap>

  <sql id="allProdutoColumns">
cdBarras
,ncm
,cEST
,produto
,quant
,excTabIPI
,origem
,produtoTipo
,medicamento
,veiculo
,armamento
,combustivel
,dataCreate
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user
,uniMed
,marca


</sql>
<sql id="allProdutoValues">
#{cdBarras},
#{ncm},
#{cEST},
#{produto},
#{quant},
#{excTabIPI.id},
#{origem.id},
#{produtoTipo.id},
#{medicamento.id},
#{veiculo.id},
#{armamento.id},
#{combustivel.id},
#{dataCreate},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser},
     #{uniMed.id},
#{marca.id}
  </sql>


 <select id="fetchAllProdutos" resultMap="ProdutoResult">
    SELECT id,
 <include refid="allProdutoColumns" />
   FROM produto ORDER BY id ASC
 </select>

 <select id="fetchProdutoById" parameterType="Integer" resultMap="ProdutoResult">
    SELECT id,
 <include refid="allProdutoColumns" />
   FROM produto WHERE id = #{fetchid}
 </select>

 <select id="fetchAllProdutosRequest" parameterType="ProdutoInquiryRequest" resultMap="ProdutoResult">

     SELECT id,<include refid="allProdutoColumns" />
           FROM produto where 0 = 0 <if test="emprId != null"> and emprId = #{emprId}</if> <if test="ncm != null"> and ncm = #{ncm}</if> ORDER BY id ASC

</select>

<select id="fetchProdutoRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM produto WHERE id IS NOT NULL <if test="emprId != null"> and emprId = emprId</if>

 </select>

 <insert id="insertProduto" parameterType="Produto" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO produto (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allProdutoColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allProdutoValues" /> )
 </insert>

 <update id="updateProduto" parameterType="Produto">
   UPDATE produto SET
<if test="excTabIPI != null">excTabIPI = #{excTabIPI.id},</if>
<if test="origem != null">origem = #{origem.id},</if>
<if test="produtoTipo != null">produtoTipo = #{produtoTipo.id},</if>
<if test="medicamento != null">medicamento = #{medicamento.id},</if>
<if test="veiculo != null">veiculo = #{veiculo.id},</if>
<if test="armamento != null">armamento = #{armamento.id},</if>
<if test="combustivel != null">combustivel = #{combustivel.id},</if>
<if test="cdBarras!= null">cdBarras = #{cdBarras},</if>
<if test="ncm!= null">ncm = #{ncm},</if>
<if test="cEST!= null">cEST = #{cEST},</if>
<if test="produto!= null">produto = #{produto},</if>
<if test="dataCreate!= null">dataCreate = #{dataCreate},</if>
<if test="quant!= null">quant = #{quant},</if>
<if test="uniMed!= null">uniMed = #{uniMed.id},</if>
<if test="marca!= null">marca = #{marca.id},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteProdutoById" parameterType="Produto">
  DELETE FROM produto WHERE id = #{id}
 </delete>

 <delete id="deleteAllProdutos">
 DELETE FROM produto
  </delete>
</mapper>


