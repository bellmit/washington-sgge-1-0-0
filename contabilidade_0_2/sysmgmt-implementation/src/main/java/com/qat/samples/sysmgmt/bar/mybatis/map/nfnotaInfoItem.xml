<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="NFNotaInfoItemMap">
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache" />
	<cache type="org.mybatis.caches.ehcache.EhcacheCache" />

	<!-- Result maps describe the mapping between the columns returned from 
		a query, and the class properties. A result map isnt necessary if the columns 
		(or aliases) match to the properties exactly. -->
	<resultMap id="NFNotaInfoItemResult" type="NFNotaInfoItem">
		<result property="id" column="id" />
		<result property="numeroItem" column="numeroItem" />

		<result property="informacoesAdicionais" column="informacoesAdicionais" />
		<result property="parentId" column="parentId" />
		<result property="tabelaEnumValue" column="tabelaEnumValue" />
		<result property="emprId" column="emprId" />
		<result property="processId" column="processId" />
		<result property="createUser" column="create_user" />
		<result property="createDateUTC" column="create_date" />
		<result property="modifyUser" column="modify_user" />
		<result property="modifyDateUTC" column="modify_date" />
		<collection property="produto" column="produto"
			select="RegimeMap.fetchRegimeById" />
		<collection property="imposto" column="imposto"
			select="RegimeMap.fetchRegimeById" />
		<collection property="impostoDevolvido" column="impostoDevolvido"
			select="RegimeMap.fetchRegimeById" />
	</resultMap>

	<sql id="allNFNotaInfoItemColumns">
		numeroItem
		,produto
		,imposto
		,impostoDevolvido
		,informacoesAdicionais
		,parentId
		,tabelaEnumValue
		,emprId
		,processId
		,create_date
		,create_user
		,modify_date
		,modify_user

	</sql>
	<sql id="allNFNotaInfoItemValues">
		#{numeroItem},
		#{produto.id},
		#{imposto.id},
		#{impostoDevolvido.id},
		#{informacoesAdicionais},
		#{parentId},
		#{tabelaEnumValue},
		#{emprId},
		#{processId},
		#{createDateUTC},
		#{createUser},
		#{modifyDateUTC},
		#{modifyUser}
	</sql>


	<select id="fetchAllNFNotaInfoItems" resultMap="NFNotaInfoItemResult">
		SELECT id,
		<include refid="allNFNotaInfoItemColumns" />
		FROM nfnotainfoitem ORDER BY id ASC
	</select>

	<select id="fetchNFNotaInfoItemById" parameterType="Integer"
		resultMap="NFNotaInfoItemResult">
		SELECT id,
		<include refid="allNFNotaInfoItemColumns" />
		FROM nfnotainfoitem WHERE id = #{fetchid}
	</select>

	<select id="fetchAllNFNotaInfoItemsRequest" parameterType="PagedInquiryRequest"
		resultMap="NFNotaInfoItemResult">

		SELECT id,
		<include refid="allNFNotaInfoItemColumns" />
		FROM nfnotainfoitem where 0 = 0
		<if test="emprId != null"> and emprId = emprId</if>
		ORDER BY id ASC
		OFFSET ( #{startPage} * #{pageSize} )
		LIMIT #{pageSize}
	</select>

	<select id="fetchNFNotaInfoItemRowCount" resultType="Integer">
		SELECT COUNT(*) AS RECORD_COUNT FROM nfnotainfoitem WHERE id IS NOT
		NULL
		<if test="emprId != null"> and emprId = emprId</if>

	</select>

	<insert id="insertNFNotaInfoItem" parameterType="NFNotaInfoItem"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO nfnotainfoitem (
		<if test="id != null">
			<if test="id > 0">id,</if>
		</if>
		<include refid="allNFNotaInfoItemColumns" />
		)
		VALUES (
		<if test="id != null">
			<if test="id > 0">#{id},</if>
		</if>
		<include refid="allNFNotaInfoItemValues" />
		)
	</insert>

	<update id="updateNFNotaInfoItem" parameterType="NFNotaInfoItem">
		UPDATE nfnotainfoitem SET
		<if test="numeroItem!= null">numeroItem = #{numeroItem},</if>
		<if test="produto!= null">produto = #{produto.id},</if>
		<if test="imposto!= null">imposto = #{imposto.id},</if>
		<if test="impostoDevolvido!= null">impostoDevolvido = #{impostoDevolvido.id},</if>
		<if test="informacoesAdicionais!= null">informacoesAdicionais = #{informacoesAdicionais},</if>
		<if test="parentId!= null"> parentId=#{parentId},</if>
		<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
		<if test="emprId!= null"> emprId=#{emprId},</if>
		<if test="processId!= null"> processId=#{processId},</if>
		<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
		<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
		WHERE
		id = #{id}
	</update>

	<delete id="deleteNFNotaInfoItemById" parameterType="NFNotaInfoItem">
		DELETE FROM nfnotainfoitem WHERE id = #{id}
	</delete>

	<delete id="deleteAllNFNotaInfoItems">
		DELETE FROM nfnotainfoitem
	</delete>
</mapper>


