<mapper namespace="NFNotaMap">
 <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
 <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

  <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="NFNotaResult" type="NFNota">
  <result property="id" column="id"/>
    <result property="identificadorLocal" column="identificadorLocal"/>
	<result property="parentId" column="parentId"/>
	<result property="tabelaEnumValue" column="tabelaEnumValue"/>
	<result property="emprId" column="emprId"/>
	<result property="processId" column="processId"/>
	<result property="createUser" column="create_user"/>
	<result property="createDateUTC" column="create_date"/>
	<result property="modifyUser" column="modify_user"/>
	<result property="modifyDateUTC" column="modify_date"/>
 	<collection property="info" column="info" select="NFNotaInfoMap.fetchNFNotaInfoById"/>
    <collection property="infoSuplementar" column="infoSuplementar" select="NFNotaInfoSuplementarMap.fetchNFNotaInfoSuplementarById"/>
    <collection property="assinatura" column="assinatura" select="DoisValorMap.fetchDoisValorById"/>
    <collection property="tipo" column="tipo" select="DoisValorMap.fetchDoisValorById"/>
  </resultMap>

  <sql id="allNFNotaColumns">
identificadorLocal
,info
,infoSuplementar
,assinatura
,tipo
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user

</sql>
<sql id="allNFNotaValues">
#{identificadorLocal},
#{info.id},
#{infoSuplementar.id},
#{assinatura.id},
#{tipo.id},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser}
  </sql>


 <select id="fetchAllNFNotas" resultMap="NFNotaResult">
    SELECT id,
 <include refid="allNFNotaColumns" />
   FROM nfnota ORDER BY id ASC
 </select>

 <select id="fetchNFNotaById" parameterType="Integer" resultMap="NFNotaResult">
    SELECT id,
 <include refid="allNFNotaColumns" />
   FROM nfnota WHERE id = #{fetchid}
 </select>

 <select id="fetchAllNFNotasRequest" parameterType="PagedInquiryRequest" resultMap="NFNotaResult">

     SELECT id,<include refid="allNFNotaColumns" />
           FROM nfnota where 0 = 0 <if test="permissaoTypeEnumValue == 1">
			and id IS NOT NULL
		</if>
		<if test="permissaoTypeEnumValue != 1">
			<if test="emprId == null">
				<if test="emprIds != null"> and emprId in

				<foreach item="item" index="index" collection="emprIds"
		             open="(" separator="," close=")">
		        #{item}
		    	</foreach>

				</if>
			</if>
			<if test="emprId != null"> and emprId = #{emprId}</if>
		</if> ORDER BY create_date ASC

</select>

<select id="fetchNFNotaRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM nfnota WHERE id IS NOT NULL <if test="permissaoTypeEnumValue == 1">
			and id IS NOT NULL
		</if>
		<if test="permissaoTypeEnumValue != 1">
			<if test="emprId == null">
				<if test="emprIds != null"> and emprId in

				<foreach item="item" index="index" collection="emprIds"
		             open="(" separator="," close=")">
		        #{item}
		    	</foreach>

				</if>
			</if>
			<if test="emprId != null"> and emprId = #{emprId}</if>
		</if>

 </select>

 <insert id="insertNFNota" parameterType="NFNota" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO nfnota (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allNFNotaColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allNFNotaValues" /> )
 </insert>

 <update id="updateNFNota" parameterType="NFNota">
   UPDATE nfnota SET
<if test="identificadorLocal!= null">identificadorLocal = #{identificadorLocal},</if>
<if test="info!= null">info = #{info.id},</if>
<if test="infoSuplementar!= null">infoSuplementar = #{infoSuplementar.id},</if>
<if test="assinatura!= null">assinatura = #{assinatura.id},</if>
<if test="tipo!= null">tipo = #{tipo.id},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteNFNotaById" parameterType="NFNota">
  DELETE FROM nfnota WHERE id = #{id}
 </delete>

 <delete id="deleteAllNFNotas">
 DELETE FROM nfnota
  </delete>
</mapper>


