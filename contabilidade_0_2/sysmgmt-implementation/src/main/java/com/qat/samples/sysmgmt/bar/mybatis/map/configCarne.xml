<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ConfigCarneMap">
 <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
 <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

  <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="ConfigCarneResult" type="ConfigCarne">
    <result property="id" column="id"/>

<result property="parentId" column="parentId"/>
<result property="tabelaEnumValue" column="tabelaEnumValue"/>
<result property="emprId" column="emprId"/>
<result property="processId" column="processId"/>
<result property="createUser" column="create_user"/>
<result property="createDateUTC" column="create_date"/>
<result property="modifyUser" column="modify_user"/>
<result property="modifyDateUTC" column="modify_date"/>

<result property="carneBotelo" column="carneBotelo"  typeHandler="BooleanHandler"/>
<result property="carneNormal" column="carneNormal" typeHandler="BooleanHandler"/>
  </resultMap>

  <sql id="allConfigCarneColumns">
carneBotelo
,carneNormal
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user

</sql>
<sql id="allConfigCarneValues">
#{carneBotelo.id},
#{carneNormal.id},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser}
  </sql>


 <select id="fetchAllConfigCarnes" resultMap="ConfigCarneResult">
    SELECT id,
 <include refid="allConfigCarneColumns" />
   FROM configcarne ORDER BY id ASC
 </select>

 <select id="fetchConfigCarneById" parameterType="Integer" resultMap="ConfigCarneResult">
    SELECT id,
 <include refid="allConfigCarneColumns" />
   FROM configcarne WHERE id = #{fetchid}
 </select>

 <select id="fetchAllConfigCarnesRequest" parameterType="PagedInquiryRequest" resultMap="ConfigCarneResult">

     SELECT id,<include refid="allConfigCarneColumns" />
           FROM configcarne where 0 = 0 <if test="emprId != null"> and emprId = emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

<select id="fetchConfigCarneRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM configcarne WHERE id IS NOT NULL <if test="emprId != null"> and emprId = emprId</if>

 </select>

 <insert id="insertConfigCarne" parameterType="ConfigCarne" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO configcarne (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allConfigCarneColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allConfigCarneValues" /> )
 </insert>

 <update id="updateConfigCarne" parameterType="ConfigCarne">
   UPDATE configcarne SET
<if test="carneBotelo!= null">carneBotelo = #{carneBotelo.id},</if>
<if test="carneNormal!= null">carneNormal = #{carneNormal.id},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteConfigCarneById" parameterType="ConfigCarne">
  DELETE FROM configcarne WHERE id = #{id}
 </delete>

 <delete id="deleteAllConfigCarnes">
 DELETE FROM configcarne
  </delete>
</mapper>


