<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="DoisValorMap">
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache" />
    <cache type="org.mybatis.caches.ehcache.EhcacheCache" />

    <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
    <resultMap id="DoisValorResult" type="DoisValor">
        <result property="id" column="id" />
        <result property="nome" column="nome" />
        <result property="descricao" column="descricao" />
        <result property="parentId" column="parentId" />
        <result property="tabelaEnumValue" column="tabelaEnumValue" />
        <result property="emprId" column="emprId" />
        <result property="processId" column="processId" />
        <result property="createUser" column="create_user" />
        <result property="createDateUTC" column="create_date" />
        <result property="modifyUser" column="modify_user" />
        <result property="modifyDateUTC" column="modify_date" />
    </resultMap>

    <sql id="allDoisValorColumns">
        nome ,descricao ,parentId ,tabelaEnumValue ,emprId ,processId ,create_date ,create_user ,modify_date ,modify_user

    </sql>
    <sql id="allDoisValorValues">
        #{nome}, #{descricao}, #{parentId}, #{tabelaEnumValue}, #{emprId}, #{processId}, #{createDateUTC}, #{createUser}, #{modifyDateUTC}, #{modifyUser}
    </sql>


    <select id="fetchAllDoisValors" resultMap="DoisValorResult">
    SELECT id,
 <include refid="allDoisValorColumns" />
   FROM doisvalor ORDER BY id ASC
 </select>

    <select id="fetchDoisValorById" parameterType="Integer" resultMap="DoisValorResult">
    SELECT id,
 <include refid="allDoisValorColumns" />
   FROM doisvalor WHERE id = #{fetchid}
 </select>

    <select id="fetchAllDoisValorsRequest" parameterType="DoisValorInquiryRequest" resultMap="DoisValorResult">

     SELECT id,<include refid="allDoisValorColumns" />
           FROM doisvalor where 0 = 0 <if test="criteria.emprId != null"> and emprId = criteria.emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

    <select id="fetchDoisValorRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM doisvalor WHERE id IS NOT NULL <if test="criteria.emprId != null"> and emprId = criteria.emprId</if>

 </select>

    <insert id="insertDoisValor" parameterType="DoisValor" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO doisvalor (
        <if test="id != null">
            <if test="id > 0">id,</if>
        </if>
        <include refid="allDoisValorColumns" /> ) VALUES (
        <if test="id != null">
            <if test="id > 0">#{id},</if>
        </if>
        <include refid="allDoisValorValues" /> )
    </insert>

    <update id="updateDoisValor" parameterType="DoisValor">
        UPDATE doisvalor SET
        <if test="nome!= null">nome = #{nome},</if>
        <if test="descricao!= null">descricao = #{descricao},</if>
        <if test="parentId!= null"> parentId=#{parentId},</if>
        <if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
        <if test="emprId!= null"> emprId=#{emprId},</if>
        <if test="processId!= null"> processId=#{processId},</if>
        <if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
        <if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
        WHERE id = #{id}
    </update>

    <delete id="deleteDoisValorById" parameterType="DoisValor">
        DELETE FROM doisvalor WHERE id = #{id}
    </delete>

    <delete id="deleteAllDoisValors">
        DELETE FROM doisvalor
    </delete>
</mapper>