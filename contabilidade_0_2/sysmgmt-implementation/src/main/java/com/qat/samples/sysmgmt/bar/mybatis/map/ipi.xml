<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="IpiMap">
 <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
 <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

  <!-- Result maps describe the mapping between the columns returned
      from a query, and the class properties.  A result map isnt
      necessary if the columns (or aliases) match to the properties
       exactly. -->
  <resultMap id="IpiResult" type="Ipi">
    <result property="id" column="id"/>
    <result property="prodId" column="prodId"/>
    
    <result property="classeCigarrosBebidas" column="classeCigarrosBebidas"/>
    <result property="cNPJProdutor" column="cNPJProdutor"/>
    <result property="codControleIPI" column="codControleIPI"/>
    <result property="qtdSeloIPI" column="qtdSeloIPI"/>

    <result property="aliquotaIPI" column="aliquotaIPI"/>
<result property="parentId" column="parentId"/>
<result property="tabelaEnumValue" column="tabelaEnumValue"/>
<result property="emprId" column="emprId"/>
<result property="processId" column="processId"/>
<result property="createUser" column="create_user"/>
<result property="createDateUTC" column="create_date"/>
<result property="modifyUser" column="modify_user"/>
<result property="modifyDateUTC" column="modify_date"/>
<collection property="sitTributaria" column="sitTributaria" select="DoisValorMap.fetchDoisValorById"/>
<collection property="codEnquadramento" column="codEnquadramento" select="DoisValorMap.fetchDoisValorById"/>
<collection property="tipoCalculo" column="tipoCalculo" select="DoisValorMap.fetchDoisValorById"/>
</resultMap>

  <sql id="allIpiColumns">
prodId
,sitTributaria
,classeCigarrosBebidas
,cNPJProdutor
,codControleIPI
,qtdSeloIPI
,codEnquadramento
,tipoCalculo
,aliquotaIPI
,parentId
,tabelaEnumValue
,emprId
,processId
,create_date
,create_user
,modify_date
,modify_user

</sql>
<sql id="allIpiValues">
#{prodId},
#{sitTributaria.id},
#{classeCigarrosBebidas},
#{cNPJProdutor},
#{codControleIPI},
#{qtdSeloIPI},
#{codEnquadramento.id},
#{tipoCalculo.id},
#{aliquotaIPI},
     #{parentId},
     #{tabelaEnumValue},
     #{emprId},
     #{processId},
     #{createDateUTC},
     #{createUser},
     #{modifyDateUTC},
     #{modifyUser}
  </sql>


 <select id="fetchAllIpis" resultMap="IpiResult">
    SELECT id,
 <include refid="allIpiColumns" />
   FROM ipi ORDER BY id ASC
 </select>

 <select id="fetchIpiById" parameterType="Integer" resultMap="IpiResult">
    SELECT id,
 <include refid="allIpiColumns" />
   FROM ipi WHERE id = #{fetchid}
 </select>

 <select id="fetchAllIpisRequest" parameterType="PagedInquiryRequest" resultMap="IpiResult">

     SELECT id,<include refid="allIpiColumns" />
           FROM ipi where 0 = 0 <if test="emprId != null"> and emprId = emprId</if> ORDER BY id ASC
       OFFSET ( #{startPage} * #{pageSize} )
       LIMIT #{pageSize}
</select>

<select id="fetchIpiRowCount" resultType="Integer">
     SELECT COUNT(*) AS RECORD_COUNT FROM ipi WHERE id IS NOT NULL <if test="emprId != null"> and emprId = emprId</if>

 </select>

 <insert id="insertIpi" parameterType="Ipi" useGeneratedKeys="true" keyProperty="id">
     INSERT INTO ipi (
 <if test="id != null"><if test="id > 0">id,</if></if><include refid="allIpiColumns" />
     )
   VALUES (<if test="id != null"><if test="id > 0">#{id},</if></if><include refid="allIpiValues" /> )
 </insert>

 <update id="updateIpi" parameterType="Ipi">
   UPDATE ipi SET
<if test="prodId!= null">prodId = #{prodId},</if>
<if test="sitTributaria!= null">sitTributaria = #{sitTributaria.id},</if>
<if test="classeCigarrosBebidas!= null">classeCigarrosBebidas = #{classeCigarrosBebidas},</if>
<if test="cNPJProdutor!= null">cNPJProdutor = #{cNPJProdutor},</if>
<if test="codControleIPI!= null">codControleIPI = #{codControleIPI},</if>
<if test="qtdSeloIPI!= null">qtdSeloIPI = #{qtdSeloIPI},</if>
<if test="codEnquadramento!= null">codEnquadramento = #{codEnquadramento.id},</if>
<if test="tipoCalculo!= null">tipoCalculo = #{tipoCalculo.id},</if>
<if test="aliquotaIPI!= null">aliquotaIPI = #{aliquotaIPI},</if>
<if test="parentId!= null"> parentId=#{parentId},</if>
<if test="tabelaEnumValue!= null"> tabelaEnumValue=#{tabelaEnumValue},</if>
<if test="emprId!= null"> emprId=#{emprId},</if>
<if test="processId!= null"> processId=#{processId},</if>
<if test="modifyDateUTC!= null"> modify_date=#{modifyDateUTC},</if>
<if test="modifyUser!= null"> modify_user=#{modifyUser}</if>
   WHERE
    id = #{id}
 </update>

<delete id="deleteIpiById" parameterType="Ipi">
  DELETE FROM ipi WHERE id = #{id}
 </delete>

 <delete id="deleteAllIpis">
 DELETE FROM ipi
  </delete>
</mapper>


